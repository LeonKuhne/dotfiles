#!/bin/sh
function colors() {
  for x in {0..8}; do
    for i in {30..37}; do
      for a in {40..47}; do
        echo -ne "\e[$x;$i;$a""m\\\e[$x;$i;$a""m\e[0;37;40m ";
      done;
      echo;
    done;
  done;
}


# custom shell agnostic color aliases

export C_PRI= # primary theme
export C_ALT= # alternative theme
